<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="icon_style" parent="Theme.DDI_Tarea7">
        <item name="cornerRadius">4dp</item>
        <item name="strokeColor">@color/md_theme_primary</item>
        <item name="android:textColor">@color/md_theme_primary</item>
        <item name="backgroundTint">@color/md_theme_inverseOnSurface</item>
        <item name="textFillColor">@color/md_theme_primary</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="Icon_style_2" parent="Theme.DDI_Tarea7">
        <item name="cornerRadius">4dp</item>
        <item name="backgroundTint">@color/md_theme_primary</item>
    </style>

    <style name="icon_style_3" parent="Base.Theme.DDI_Tarea7">
        <item name="backgroundTint">@color/md_theme_outlineVariant_mediumContrast</item>
    </style>
    
    <style name="link" parent="Base.Theme.DDI_Tarea7">
        <item name="android:textColor">@color/blue</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?attr/selectableItemBackgroundBorderless</item>
    </style>


</resources>